    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "prettier": "^1.18.2",
    "typescript": "^3.7.2",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "keywords": [
